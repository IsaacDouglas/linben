<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.samsung.linben">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".Causa1Activity" />
        <activity android:name=".ApeloActivity" />
        <activity android:name=".ConfPerfilActivity" />
        <activity android:name=".AlertAproxChaveiroActivity" />
        <activity android:name=".AlertAcessoNegadoActivity" />
        <activity android:name=".AlertWifiActivity" />
        <activity android:name=".ListUsersActivity" />
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".ContinuarCadastroActivity"
            android:label="@string/title_activity_continuar_cadastro"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".DoacaoRegistradaActivity"></activity>
    </application>

</manifest>